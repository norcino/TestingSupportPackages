<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Object.Builder</PackageId>
    <Authors>Manuel Salvatori</Authors>
    <Company />
    <Description>Builder allows to initialize one or more objects customizing the creation.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/norcino/TestingSupportPackages/tree/master/Builder</PackageProjectUrl>
    <PackageIcon>Logo128.png</PackageIcon>
    <PackageTags>test, testing, stub, mock, factory, builder, instance</PackageTags>
    <PackageReleaseNotes>Fixed issue affecting new Exclude feature.</PackageReleaseNotes>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="Logo128.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AnonymousData" Version="1.0.2" />
  </ItemGroup>

</Project>
